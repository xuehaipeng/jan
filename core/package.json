    "@vitest/ui": "^2.1.8",
    "eslint": "8.57.0",
    "happy-dom": "^15.11.6",
    "pacote": "^21.0.0",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "rimraf": "^6.0.1",
    "rolldown": "1.0.0-beta.1",
    "tslib": "^2.6.2",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "rxjs": "^7.8.1",
    "ulidx": "^2.3.0"
  },
  "packageManager": "yarn@4.5.3"

{
  "name": "@janhq/core",
  "version": "0.1.10",
  "description": "Core library for the Jan AI application framework",
  "keywords": [
    "jan",
    "core"
  ],
  "homepage": "https://jan.ai",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "author": "Jan <service@jan.ai>",
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
